{
  "name": "zenobia-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:vite": "vite --port 3000",
    "dev:worker": "wrangler dev src/worker.ts --local --port 8787",
    "dev": "npm run build && npx wrangler dev",
    "build": "tsc && vite build",
    "deploy": "npm run build && wrangler deploy",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.ts",
    "generate-api": "tsx utils/generate-api.ts"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.3",
    "@cloudflare/vite-plugin": "^1.2.4",
    "@solidjs/router": "^0.15.2",
    "graphql-request": "^7.2.0",
    "jose": "^6.0.11",
    "qrcode": "^1.5.4",
    "solid-js": "^1.9.5",
    "solid-toast": "^0.5.0",
    "vite-plugin-solid": "^2.11.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250510.0",
    "@graphql-codegen/cli": "5.0.6",
    "@graphql-codegen/client-preset": "4.8.1",
    "@graphql-codegen/typed-document-node": "^5.1.1",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@octokit/rest": "^22.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "constructs": "^10.3.0",
    "daisyui": "^5.0.9",
    "dotenv": "^17.0.1",
    "esbuild": "^0.25.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "graphql": "^16.11.0",
    "js-yaml": "^4.1.0",
    "openapi-typescript": "^7.4.0",
    "plaid": "^30.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^6.1.0",
    "wrangler": "^4.17.0"
  }
}
